{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts",
        "**/*.cjs",
        "**/*.mjs"
    ],
    "exclude": ["node_modules"]
}
